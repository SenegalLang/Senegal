var senegal = Map();
senegal.add("age", 10);
senegal["name"] = "Mel";

println(senegal["age"]); // output 10
println(senegal["name"]); // output Mel

println(senegal.contains("age")); // output true
senegal.remove("age");
println(senegal.contains("age")); // output false

println(senegal.length()); // output 1

senegal.clear();
println(senegal.isEmpty()); // output true

// Non string keys

var nonStringKey = {12: "foo"};
println(nonStringKey[12]); // output foo

var mapString = nonStringKey.toString();
println(mapString); // output {12: foo}
println(nonStringKey); // output {12: foo}