enhance List {
    function testForEach(fun) {

        for (var i = 0; i < this.length(); i++)
            fun(this, i, this[i]);
    }
}

function removeZero(list, i, num) {
    if (num == 0)
        list.removeAt(i);
}

var list = [19, 12, 0, 123, 0];

list.testForEach(removeZero);

println(list); // output [19,12,123]