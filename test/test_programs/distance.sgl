function distance(x1, x2, y1, y2) {
    println(x1);
    println(x2);
    println(y1);
    println(y2);

    var x = (x2 - x1) ** 2;
    var y = (y2 - y1) ** 2;

    println(x);
    return (x + y) ** 0.5;
}

